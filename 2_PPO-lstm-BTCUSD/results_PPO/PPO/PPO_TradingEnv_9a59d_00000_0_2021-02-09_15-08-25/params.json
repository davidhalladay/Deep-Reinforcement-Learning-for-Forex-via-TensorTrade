{
  "clip_rewards": true,
  "eager_tracing": true,
  "entropy_coeff": 0.01,
  "env": "TradingEnv",
  "env_config": {
    "window_size": 25
  },
  "framework": "torch",
  "gamma": 0,
  "ignore_worker_failures": true,
  "lambda": 0.72,
  "log_level": "DEBUG",
  "lr": 8e-06,
  "model": {
    "fcnet_activation": "relu",
    "fcnet_hiddens": [
      256,
      256
    ],
    "lstm_cell_size": 256,
    "lstm_use_prev_action": true,
    "lstm_use_prev_reward": true,
    "max_seq_len": 40,
    "use_lstm": true
  },
  "num_gpus": 0,
  "num_workers": 1,
  "observation_filter": "MeanStdFilter",
  "vf_loss_coeff": 0.5
}